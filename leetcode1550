class Solution {
    public boolean threeConsecutiveOdds(int[] arr) {
        
      int n=arr.length;
       for(int i=0;i<n-2;i++)
        {
            if(arr[i]%2==0)
            continue;
         for(int j=i+1;i<n-1;i++)
            {
            if(arr[j]%2!=0&&arr[j+1]%2!=0)
            return true;
            else
            break;
             }
        }
         return false;
         
        
    }
}